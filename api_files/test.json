  "paths": {
    "/eta/booking": {
      "post": {
        "tags": [
          "default"
        ],
        "operationId": "post_restful_booking_with_api_key",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/restful_booking_model"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success call to data source. Data source provides details for all containers related to that booking number.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "containers": {
                      "type": "array",
                      "description": "List of containers.",
                      "items": {
                        "type": "string"
                      }
                    },
                    "status": {
                      "type": "string",
                      "description": "Request status. can be either success or error."
                    },
                    "message": {
                      "type": "string",
                      "description": "Request description."
                    },
                    "response": {
                      "type": "array",
                      "description": "List of containers and it's details including bookmark ID.",
                      "items": {
                        "type": "object",
                        "properties": {}
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Payload details do not match API requirements.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Invalid message details."
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "API Quota exceeded.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Quota exceeded message."
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error/ Unexpect error.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Quota exceeded message."
                    }
                  }
                }
              }
            }
          }
        },
        "x-codegen-request-body-name": "payload"
      }
    }
  },
  "components": {
    "schemas": {
      "restful_booking_model": {
        "required": [
          "carrier_no",
          "doc_no",
          "doc_type"
        ],
        "type": "object",
        "properties": {
          "doc_type": {
            "type": "string",
            "example": "BK"
          },
          "doc_no": {
            "type": "string",
            "example": "MAEU969678061"
          },
          "carrier_no": {
            "type": "string",
            "example": "CMDU"
          }
        }
      }
    },
    "responses": {
      "MaskError": {
        "description": "When any error occurs on mask",
        "content": {}
      },
      "ParseError": {
        "description": "When a mask can't be parsed",
        "content": {}
      }
    },
    "securitySchemes": {
      "authorization": {
        "type": "apiKey",
        "name": "authorization",
        "in": "header"
      },
      "x-api-key": {
        "type": "apiKey",
        "name": "x-api-key",
        "in": "header"
      },
      "x-org-api-key": {
        "type": "apiKey",
        "name": "x-org-api-key",
        "in": "header"
      }
    }
